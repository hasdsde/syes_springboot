<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.syes.syes_springboot.mapper.RollMapper">

    <select id="selectAll" resultType="com.syes.syes_springboot.entity.Roll">
        select *
        from roll
    </select>
    <select id="selectEnable" resultType="com.syes.syes_springboot.entity.Roll">
        select *
        from roll
        where enable = 1
    </select>
</mapper>
